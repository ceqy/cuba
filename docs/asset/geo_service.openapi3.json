              "type": "string"
            }
          },
          {
            "name": "context.user_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.roles",
            "in": "query",
            "required": false,
            "explode": true,
