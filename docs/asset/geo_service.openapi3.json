            "schema": {
              "type": "string"
            }
          }
        ],
        "tags": [
          "GeoService"
        ]
      }
    },
    "/api/v1/asset/route/calculate": {
      "post": {
        "summary": "计算两个地理点之间的路线 - 异步",
        "operationId": "GeoService_CalculateRoute",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/commonJobInfo"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/geoCalculateRouteRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "GeoService"
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "commonApiMessage": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "code": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "target": {
            "type": "string"
          }
        }
      },
      "commonJobInfo": {
        "type": "object",
        "properties": {
          "job_id": {
            "type": "string"
          },
          "job_type": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/commonJobStatus"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "started_at": {
            "type": "string",
            "format": "date-time"
          },
          "completed_at": {
            "type": "string",
            "format": "date-time"
          },
          "progress_percentage": {
            "type": "integer",
            "format": "int32"
          },
          "messages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/commonApiMessage"
            }
          },
          "error_detail": {
            "type": "string"
          }
        }
      },
      "commonJobStatus": {
        "type": "string",
        "enum": [
          "JOB_STATUS_UNSPECIFIED",
          "JOB_STATUS_PENDING",
          "JOB_STATUS_RUNNING",
          "JOB_STATUS_COMPLETED",
          "JOB_STATUS_FAILED",
          "JOB_STATUS_CANCELLED",
          "JOB_STATUS_PARTIALLY_COMPLETED"
        ],
        "default": "JOB_STATUS_UNSPECIFIED",
        "title": "异步作业状态"
      },
      "commonRequestContext": {
        "type": "object",
        "properties": {
          "request_id": {
            "type": "string"
          },
          "tenant_id": {
            "type": "string"
          },
          "user_id": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "idempotency_key": {
            "type": "string"
          },
          "locale": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "source_system": {
            "type": "string"
          },
          "correlation_id": {
            "type": "string"
          }
        }
      },
      "commonTravelMode": {
        "type": "string",
        "enum": [
          "TRAVEL_MODE_UNSPECIFIED",
          "TRAVEL_MODE_DRIVING",
          "TRAVEL_MODE_WALKING",
          "TRAVEL_MODE_BICYCLING",
          "TRAVEL_MODE_TRANSIT"
        ],
        "default": "TRAVEL_MODE_UNSPECIFIED",
        "title": "交通方式 (Geo)"
      },
      "geoAssetLocation": {
        "type": "object",
        "properties": {
          "asset_id": {
            "type": "string"
          },
          "location": {
            "$ref": "#/components/schemas/geoGeoPoint"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "title": "资产位置"
      },
      "geoCalculateRouteRequest": {
        "type": "object",
        "properties": {
          "start": {
            "$ref": "#/components/schemas/geoGeoPoint"
          },
          "end": {
            "$ref": "#/components/schemas/geoGeoPoint"
          },
          "travel_mode": {
            "$ref": "#/components/schemas/commonTravelMode"
          },
          "context": {
            "$ref": "#/components/schemas/commonRequestContext"
          }
        },
        "title": "计算路线请求"
      },
      "geoCheckGeofenceRequest": {
        "type": "object",
        "properties": {
          "asset_id": {
            "type": "string"
          },
          "fence_id": {
            "type": "string"
          },
          "context": {
            "$ref": "#/components/schemas/commonRequestContext"
          }
        },
        "title": "检查地理围栏请求"
      },
      "geoCheckGeofenceResponse": {
        "type": "object",
        "properties": {
          "is_inside": {
            "type": "boolean"
          }
        }
      },
      "geoCreateGeofenceRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "vertices": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/geoGeoPoint"
            }
          },
          "description": {
            "type": "string"
          },
          "context": {
            "$ref": "#/components/schemas/commonRequestContext"
          }
        }
      },
      "geoGeoPoint": {
        "type": "object",
        "properties": {
          "latitude": {
            "type": "number",
            "format": "double"
          },
          "longitude": {
            "type": "number",
            "format": "double"
          }
        },
        "title": "地理坐标"
      },
      "geoGeofenceResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "fence_id": {
            "type": "string"
          }
        }
      },
      "geoRoute": {
        "type": "object",
        "properties": {
          "distance_meters": {
            "type": "number",
            "format": "double"
          },
          "duration_seconds": {
            "type": "string",
            "format": "int64"
          },
          "steps": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/geoRouteStep"
            }
          }
        },
        "title": "路线"
      },
      "geoRouteStep": {
        "type": "object",
        "properties": {
          "instruction": {
            "type": "string"
          },
          "distance_meters": {
            "type": "number",
            "format": "double"
          }
        },
        "title": "路线步骤"
      },
      "geoUpdateAssetLocationRequest": {
        "type": "object",
        "properties": {
          "asset_id": {
            "type": "string"
          },
          "location": {
            "$ref": "#/components/schemas/geoGeoPoint"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          },
          "context": {
            "$ref": "#/components/schemas/commonRequestContext"
          }
        }
      },
      "geoUpdateLocationResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "$ref": "#/components/schemas/commonApiMessage"
          }
        }
      },
      "protobufAny": {
        "type": "object",
        "properties": {
          "@type": {
            "type": "string"
          }
        },
        "additionalProperties": {}
      },
      "rpcStatus": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "details": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/protobufAny"
            }
          }
        }
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "Enter your JWT token (e.g. from Login API)"
      }
    }
  },
  "security": [
    {
      "BearerAuth": []
    }
  ]
}