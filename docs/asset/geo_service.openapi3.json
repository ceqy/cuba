                "type": "string"
              }
            }
          },
          {
            "name": "context.idempotency_key",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.locale",
            "in": "query",
            "required": false,
