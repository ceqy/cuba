{
  "openapi": "3.1.0",
  "info": {
    "title": "Geolocation Service API",
    "version": "version not set",
    "description": "地理位置服务 - 地理编码、路径规划、距离计算",
    "contact": {
      "name": "Platform Team",
      "email": "platform@enterprise.com"
    }
  },
  "tags": [
    {
      "name": "GeoService",
      "description": "GeoService"
    }
  ],
  "paths": {
    "/api/v1/asset/asset-location": {
      "get": {
        "summary": "获取资产的最后已知位置",
        "operationId": "GeoService_GetAssetLocation",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/geoAssetLocation"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "asset_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.request_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.tenant_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.user_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.roles",
            "in": "query",
            "required": false,
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "context.idempotency_key",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.locale",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.timestamp",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "context.source_system",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.correlation_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "tags": [
          "GeoService"
        ]
      },
      "put": {
        "summary": "更新资产位置 (New)",
        "operationId": "GeoService_UpdateAssetLocation",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/geoUpdateLocationResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/geoUpdateAssetLocationRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "GeoService"
        ]
      }
    },
    "/api/v1/asset/geofence": {
      "post": {
        "summary": "创建地理围栏 (New)",
        "operationId": "GeoService_CreateGeofence",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/geoGeofenceResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/geoCreateGeofenceRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "GeoService"
        ]
      }
    },
    "/api/v1/asset/geofence/check": {
      "post": {
        "summary": "检查资产是否在指定的地理围栏内",
        "operationId": "GeoService_CheckGeofence",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/geoCheckGeofenceResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/geoCheckGeofenceRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "GeoService"
        ]
      }
    },
    "/api/v1/asset/route-result": {
      "get": {
        "summary": "获取路线计算结果 (New)",
        "operationId": "GeoService_GetRouteResult",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/geoRoute"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "job_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.request_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.tenant_id",
            "in": "query",
