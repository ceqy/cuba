          "document_type": {
            "type": "string"
          },
          "document_category": {
            "type": "string"
          }
        }
      },
      "invoiceInvoiceDetail": {
        "type": "object",
        "properties": {
          "internal_invoice_id": {
            "type": "string",
            "title": "内部发票ID"
          },
          "document_reference": {
            "$ref": "#/components/schemas/commonSystemDocumentReference"
          },
          "status": {
            "$ref": "#/components/schemas/invoiceInvoiceProcessingStatus"
          },
          "header": {
            "$ref": "#/components/schemas/invoiceInvoiceHeader"
          },
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/invoiceInvoiceItem"
            },
            "title": "发票行项目"
          }
        },
        "title": "完整的发票详情"
      },
      "invoiceInvoiceHeader": {
        "type": "object",
        "properties": {
          "company_code": {
            "type": "string",
            "title": "公司代码 (原: BUKRS)"
          },
          "document_date": {
            "type": "string",
            "format": "date-time",
            "title": "发票凭证日期 (原: BLDAT)"
          },
          "posting_date": {
            "type": "string",
            "format": "date-time",
            "title": "过账日期 (原: BUDAT)"
          },
          "supplier_invoice_number": {
            "type": "string",
            "title": "供应商发票号 (原: XBLNR)"
          },
          "gross_amount": {
            "$ref": "#/components/schemas/commonMonetaryValue"
          },
          "tax_amount": {
            "$ref": "#/components/schemas/commonMonetaryValue"
          },
          "currency": {
            "type": "string",
            "title": "货币代码 (原: WAERS)"
          },
          "payment_terms": {
            "type": "string",
            "title": "付款条件 (原: ZTERM)"
          },
          "header_text": {
            "type": "string",
            "title": "抬头文本 (原: BKTXT)"
          }
        },
        "title": "发票抬头 (参考: RBKP)"
      },
      "invoiceInvoiceItem": {
        "type": "object",
        "properties": {
          "item_number": {
            "type": "integer",
            "format": "int32",
            "title": "发票行项目号 (原: BUZEI)"
          },
          "purchase_order_number": {
            "type": "string",
            "title": "采购订单号 (原: EBELN)"
          },
          "purchase_order_item": {
            "type": "integer",
            "format": "int32",
            "title": "采购订单行项目 (原: EBELP)"
          },
          "material": {
            "type": "string",
            "title": "物料编号 (原: MATNR)"
          },
          "short_text": {
            "type": "string",
            "title": "短文本 (原: TXZ01)"
          },
          "quantity": {
            "$ref": "#/components/schemas/commonQuantityValue"
          },
          "unit": {
            "type": "string",
            "title": "单位 (原: MEINS)"
          },
          "amount": {
            "$ref": "#/components/schemas/commonMonetaryValue"
          },
          "tax_code": {
            "type": "string",
            "title": "税码 (原: MWSKZ)"
          },
          "goods_receipt_document": {
            "type": "string",
            "title": "收货凭证 (原: LFBNR)"
          },
          "goods_receipt_year": {
            "type": "integer",
            "format": "int32",
            "title": "收货凭证年度 (原: LFGJA)"
          },
          "goods_receipt_item": {
            "type": "integer",
            "format": "int32",
            "title": "收货凭证行项目 (原: LFPOS)"
          }
        },
        "title": "发票行项目 (参考: RSEG)"
      },
      "invoiceInvoiceProcessingStatus": {
        "type": "string",
        "enum": [
          "INVOICE_PROCESSING_STATUS_UNSPECIFIED",
          "RECEIVED",
          "MATCHING",
          "MATCH_SUCCESS",
          "MATCH_FAILURE",
          "POSTED",
          "PARKED",
          "CANCELLED",
          "REVERSED"
        ],
        "default": "INVOICE_PROCESSING_STATUS_UNSPECIFIED",
        "title": "- RECEIVED: 已接收，待处理\n - MATCHING: 匹配中\n - MATCH_SUCCESS: 匹配成功\n - MATCH_FAILURE: 匹配失败，存在差异\n - POSTED: 已过账\n - PARKED: 已冻结/暂存\n - CANCELLED: 已取消\n - REVERSED: 已冲销"
      },
      "invoiceReceiveInvoiceRequest": {
        "type": "object",
        "properties": {
          "header": {
            "$ref": "#/components/schemas/invoiceInvoiceHeader"
          },
          "items": {
            "type": "array",
