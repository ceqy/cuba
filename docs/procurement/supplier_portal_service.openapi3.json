{
  "openapi": "3.1.0",
  "info": {
    "title": "Supplier Portal Service API",
    "version": "version not set",
    "description": "供应商门户服务 - PO确认、ASN发送、协同管理",
    "contact": {
      "name": "Supplier Relations Team",
      "email": "supplier@enterprise.com"
    }
  },
  "tags": [
    {
      "name": "SupplierPortalService",
      "description": "SupplierPortalService"
    }
  ],
  "paths": {
    "/api/v1/procurement/invoice/submit": {
      "post": {
        "summary": "供应商提交发票",
        "operationId": "SupplierPortalService_SubmitInvoice",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/invoiceReceiveInvoiceResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/invoiceReceiveInvoiceRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "SupplierPortalService"
        ]
      }
    },
    "/api/v1/procurement/my-invoice-status": {
      "get": {
        "summary": "供应商查询其发票状态",
        "operationId": "SupplierPortalService_GetMyInvoiceStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/invoiceInvoiceDetail"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "supplier_id",
            "description": "供应商ID",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "supplier_invoice_number",
            "description": "供应商的发票号",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.request_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.tenant_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.user_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.roles",
            "in": "query",
            "required": false,
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "context.idempotency_key",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.locale",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.timestamp",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "context.source_system",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.correlation_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "tags": [
          "SupplierPortalService"
        ]
      }
    },
    "/api/v1/procurement/my-purchase-orders": {
      "get": {
        "summary": "供应商查询其采购订单列表",
        "operationId": "SupplierPortalService_GetMyPurchaseOrders",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/orderListPurchaseOrdersResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "supplier_id",
            "description": "供应商ID (通常从上下文中获取，此处可作为过滤器或代理)",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "order_date_range.start_date",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "order_date_range.end_date",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "status",
            "description": "订单状态过滤器",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "RELEASE_STATUS_UNSPECIFIED",
                "RELEASE_STATUS_NOT_RELEASED",
                "RELEASE_STATUS_PARTIALLY_RELEASED",
                "RELEASE_STATUS_RELEASED",
                "RELEASE_STATUS_BLOCKED",
                "RELEASE_STATUS_REJECTED"
              ],
              "default": "RELEASE_STATUS_UNSPECIFIED"
            }
          },
          {
            "name": "purchasing_org",
            "description": "采购组织",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pagination.page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "pagination.page_size",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "pagination.sort_by",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pagination.descending",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "context.request_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.tenant_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.user_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.roles",
            "in": "query",
            "required": false,
            "explode": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "context.idempotency_key",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.locale",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.timestamp",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "context.source_system",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "context.correlation_id",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "tags": [
          "SupplierPortalService"
        ]
      }
    },
    "/api/v1/procurement/request-master-data-update": {
      "post": {
        "summary": "供应商请求更新其主数据（如地址、联系方式）",
        "operationId": "SupplierPortalService_RequestMasterDataUpdate",
        "responses": {
          "200": {
            "description": "A successful response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/supplierportalUpdateSupplierDataResponse"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/rpcStatus"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/supplierportalUpdateSupplierDataRequest"
              }
            }
          },
          "required": true
        },
        "tags": [
          "SupplierPortalService"
        ]
      }
    }
  },
